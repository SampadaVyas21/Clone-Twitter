<h1>Followings</h1>
<table class="table"> 
   <thead class="thead-dark">
      <tr>
         <th scope="col">Email</th>
      </tr>
  </thead>
  <tbody>
      <% User.all.each do |user| %>
      <tr>
         <% if current_user.followees.include?(user) %>
            <td><%= user.name %></td>
            <td>  <%= link_to "Unfollow",unfollow_user_path(user), class: "btn btn-outline-primary" %></td>
         <%end%>
      <% end %></tr>
      <%# end %>
   </tbody>
</table>
